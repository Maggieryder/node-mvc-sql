<%- include('includes/head.ejs') %>
<link rel="stylesheet" href="/css/user-list.css">
</head>
<body>
    <%- include('includes/nav.ejs') %>
    <main>
        <h1>You are now in users page!</h1>
        <% if (users.length > 0) { %>
            <ul class="user-list">
                <% users.forEach(user => { %>
                    <li><%= user.username %></li>
                <% }) %>
            </ul>
        <% } else { %>
            <h2>No users found!</h2>
        <% } %>
    </main>
<%- include('includes/end.ejs') %>
